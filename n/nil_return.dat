5000|$|MODULE Lists; (*** declare global constants, {{types and}} {{variables}} ***) TYPE List* = POINTER TO ListNode; ListNode = RECORD value : Integer; next : List; END; (*** declare procedures ***) PROCEDURE (l : List) Add* (v : Integer); BEGIN IF l = NIL THEN NEW(l); (* create record instance *) l.value := v ELSE l.next.Add(v) (* recursive call to [...]add(n) *) END END Add; PROCEDURE (l : List) Get* (...) : Integer; VAR v : Integer; BEGIN IF l = <b>NIL</b> THEN <b>RETURN</b> 0 (* [...]get (...) must always return an INTEGER *) ELSE v := l.value; (* this line will crash if l is NIL *) l := l.next; RETURN v END END Get;END Lists.|$|R
